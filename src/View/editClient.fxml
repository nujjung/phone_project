      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" spacing="50.0">
         <children>
            <Button id="editBtnEdit" mnemonicParsing="false" prefHeight="40.0" prefWidth="100.0" text="적용" />
